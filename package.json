  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DanLivassan/enquete_api/issues"
  },
  "homepage": "https://github.com/DanLivassan/enquete_api#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.3",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.9",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
