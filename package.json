    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "lint-staged": "^13.0.3",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "mkdirp": "^1.0.4",
    "mongodb": "^4.11.0",
    "validator": "^13.7.0"
